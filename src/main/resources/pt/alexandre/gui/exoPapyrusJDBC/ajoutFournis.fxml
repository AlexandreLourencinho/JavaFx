<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.alexandre.gui.exoPapyrusJDBC.AjoutFournisController">
   <children>
      <VBox alignment="CENTER" layoutX="69.0" layoutY="50.0" spacing="15.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Ajout d'un fournisseur">
               <VBox.margin>
                  <Insets left="45.0" />
               </VBox.margin>
            </Label>
            <VBox spacing="30.0">
               <children>
                  <HBox spacing="50.0">
                     <children>
                        <VBox spacing="45.0">
                           <children>
                              <Label text="Nom">
                                 <VBox.margin>
                                    <Insets top="6.0" />
                                 </VBox.margin>
                              </Label>
                              <Label text="Rue" />
                              <Label text="CP/Ville" />
                              <Label text="Contact">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <VBox spacing="30.0">
                           <children>
                              <TextField fx:id="txtNom" prefHeight="31.0" prefWidth="354.0" />
                              <TextField fx:id="txtRue" prefHeight="31.0" prefWidth="354.0" />
                              <HBox spacing="15.0">
                                 <children>
                                    <TextField fx:id="txtCp" prefHeight="31.0" prefWidth="132.0" />
                                    <TextField fx:id="txtVille" prefHeight="31.0" prefWidth="223.0" />
                                 </children>
                              </HBox>
                              <TextField fx:id="txtContact" prefHeight="31.0" prefWidth="354.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox spacing="90.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#ajouter" text="Ajouter">
                           <HBox.margin>
                              <Insets left="250.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Annuler" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
